<?xml version="1.0" encoding="ISO-8859-1"?>

<!-- This file contains the assortment of attributes possessed by every actor for the
      game system. The display order of attributes is controlled via the assignment of
      an "explicit" tag with the relative position of each.
-->

<document signature="Hero Lab Data">

  <!-- Dexterity Attribute
  -->
  <thing
    id="attrDex"
    name="Dexterity"
    compset="Attribute"
    isunique="yes"
    description="Dexterity represents a Character's agility and nimbleness.">
    <fieldval field="trtAbbrev" value="Dex"/>
    <tag group="explicit" tag="1"/>
    <tag group="AttrType" tag="ActOpp"/>
    <tag group="AttrType" tag="Physical"/>
	
    </thing>

  <thing
    id="attrStr"
    name="Strength"
    compset="Attribute"
    isunique="yes"
    description="Strength represents a Character's ability to lift weight and withstand duress.">
    <fieldval field="trtAbbrev" value="Str"/>
    <tag group="explicit" tag="2"/>
    <tag group="AttrType" tag="Effect"/>
	<tag group="AttrType" tag="Physical"/>
	</thing>
	
  <thing
    id="attrBod"
    name="Body"

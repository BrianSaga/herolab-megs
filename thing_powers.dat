<!-- This file contains the assortment of skills for the game system. Note that each
      skill is linked to an associated attribute for the character. This linkage allows
      the value of the skill to easily incorporate the effects of the linked attribute
      via a script on the component.
-->

<document signature="Hero Lab Data">

  <!-- Acid Power -->
  <thing
    id="powAcid"
    name="Acid"
    compset="Power"
    isunique="yes"
    description="Description goes here">
    <fieldval field="trtAbbrev" value="Acid"/>

    <!-- If this skill should be shown as an out-of-combat skill on the TacCon, assign this tag
    <tag group="DashTacCon" tag="NonCombat"/>
    -->

	<!-- Tags describe our skill in game terms -->
	<tag group="PowerType" tag="Physical"/>
	<tag group="Range" tag="Normal"/>
	<tag group="Type" tag="Dice"/>
	<tag group="BaseCost" tag="15"/>
	
	<!-- Factor Cost -->
	<tag group="FactorCost" tag="3"/>
	
    <!-- Specify the attribute to which this skill is linked -->
    <link linkage="powAttr" thing="attrBod"/>
	<child
		entity="entMod">
	</child>	
    </thing>

